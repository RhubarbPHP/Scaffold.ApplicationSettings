{
  "name": "rhubarbphp/scaffold-application-settings",
  "description": "Stores system wide application settings in the default repository.",
  "keywords": [ "settings", "rhubarb", "php", "framework" ],
  "homepage": "http://www.rhubarbphp.com/",
  "license": "Apache-2.0",
  "autoload": {
    "psr-4": {
      "Rhubarb\\Scaffolds\\ApplicationSettings\\": "src/",
      "Rhubarb\\Scaffolds\\ApplicationSettings\\Tests\\": "tests/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/acuthbert/rhubarb"
    }
  ],
  "require": {
    "rhubarbphp/rhubarb": ">=0.9",
    "rhubarbphp/module-stem": ">=0.9"
  },
  "require-dev": {
    "phpunit/phpunit": "3.*"
  }
}


